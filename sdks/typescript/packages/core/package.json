{
  "name": "@attest-ai/core",
  "version": "0.4.2",
  "description": "Attest SDK for testing AI agents â€” protocol types, codec, trace builder, assertion DSL",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./proto": {
      "import": {
        "types": "./dist/esm/proto/index.d.ts",
        "default": "./dist/esm/proto/index.js"
      },
      "require": {
        "types": "./dist/cjs/proto/index.d.cts",
        "default": "./dist/cjs/proto/index.cjs"
      }
    }
  },
  "bin": {
    "attest": "./dist/esm/cli.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/attest-framework/attest.git",
    "directory": "sdks/typescript/packages/core"
  },
  "license": "MIT",
  "keywords": [
    "ai",
    "testing",
    "agent",
    "llm",
    "assertions",
    "attest"
  ],
  "peerDependencies": {
    "@langchain/core": ">=0.2.0"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
